{
	"name": "PL_DATA_DS_SQL_To_SNFK_DATA_DS",
	"properties": {
		"activities": [
			{
				"name": "For each table in the events",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('tables')",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy table into snowflake",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "SnowflakeV2Sink",
									"importSettings": {
										"type": "SnowflakeImportCopyCommand"
									}
								},
								"enableStaging": true,
								"stagingSettings": {
									"linkedServiceName": {
										"referenceName": "LS_BLB_EVENT_DATA",
										"type": "LinkedServiceReference"
									},
									"path": ""
								}
							},
							"inputs": [
								{
									"referenceName": "DS_SQL_EVENT_DATA",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "DS_SNFLK",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"variables": {
			"tables": {
				"type": "Array",
				"defaultValue": [
					"[\"country_region_table\"",
					"customers_table",
					"",
					"transactions_table",
					"products_table"
				]
			}
		},
		"annotations": []
	}
}